{
    "name": "amirition/utable",
    "description": "A simple plugin that shows user information on the frontend",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "Amirition\\UTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amirition\\UTable\\Tests\\Unit\\": "tests/PHPUnit/Unit/"
        }
    },
    "authors": [
        {
            "name": "Amir Arabnezhad"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.7.0",
        "brain/monkey": "2.*",
        "phpunit/phpunit": "9.5"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.4"
    }
}
